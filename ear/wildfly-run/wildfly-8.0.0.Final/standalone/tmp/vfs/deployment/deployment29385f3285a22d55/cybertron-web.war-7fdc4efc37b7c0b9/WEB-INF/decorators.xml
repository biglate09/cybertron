<?xml version="1.0" encoding="ISO-8859-1"?>

<decorators defaultdir="/WEB-INF/decorators">

  <excludes>
    <pattern>/css/*</pattern>
    <pattern>/scripts/*</pattern>
    <pattern>/images/*</pattern>
    <pattern>/bootstrap/*</pattern>
    <pattern>/datepicker/*</pattern>
    <pattern>/pdf/*</pattern>
  </excludes>
  <decorator name="landing" page="landing-decorator.jsp">
  	<pattern>/pub/page/*</pattern>
  	<pattern>WEB-INF/pub/page/*</pattern>
  	<pattern>/secure/activatedUser.html</pattern>
  	<pattern>/WEB-INF/secure/activatedUser.html</pattern>
  	<pattern>WEB-INF/pages/auth/*</pattern>
  </decorator>
  <decorator name="nomenu" page="popup-decorator.jsp">
    <pattern>/secure/member/document/download*</pattern>
    <pattern>/error/error.html</pattern>
  	<pattern>/WEB-INF/pages/error/*</pattern>
  </decorator>
  <decorator name="member" page="menu-decorator.jsp">
    <pattern>/secure/*</pattern>
    <pattern>/WEB-INF/pages/secure/*</pattern>
    <pattern>/request/*</pattern>
    <pattern>/WEB-INF/pages/request/*</pattern>
  </decorator>
</decorators>
